{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
  },
  "globals": {
    "__DEV__": true,
  },
  "rules": {
    "func-names": "off",
    "prefer-arrow-callback": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}
